<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.wdy.demo.dao.LoginDao">

    <!--用户查询-->
    <select id="getUser" resultType="com.cn.wdy.demo.entity.User" parameterType="com.cn.wdy.demo.entity.User">
        SELECT USER_NAME,USER_PASSWORD FROM SYS_USER_TD
        WHERE USER_NAME = #{uname} AND USER_PASSWORD = #{upassword}
    </select>

</mapper>

